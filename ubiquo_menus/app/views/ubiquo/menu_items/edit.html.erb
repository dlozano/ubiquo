<div class='edit_menu_item_box'>
  <h1><%= render :partial => 'title' %></h1>
  <%= render(:partial => "shared/ubiquo/feedback") %>

  <%= ubiquo_form_for(@menu_item, :url => ubiquo.menu_menu_item_path(@menu_item.menu_id, @menu_item)) do |form| %>
    <%= render :partial => 'form', :locals => {:form => form} %>
    <%= form.submit_group do %>
      <%= form.update_button %>
    <% end %>
  <% end %>

  <%= content_for :sidebar do %>
  <% end %>
</div>
